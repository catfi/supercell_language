typedef char int8;
typedef short int16;
typedef long int32;
typedef long long int64;

// no-param vs params vs return-value
void _func_00_() { }               // _Z9_func_00_v
void _func_01_(int8 x, int8 y) { } // _Z9_func_01_cc

// primitive-type
void _func_10_(int8 x) { }  // _Z9_func_10_c
void _func_11_(int16 x) { } // _Z9_func_11_s
void _func_12_(int32 x) { } // _Z9_func_12_l
void _func_13_(int32 x) { } // _Z9_func_13_l
void _func_14_(int64 x) { } // _Z9_func_14_x

// non-primitive-type
class _QWE_ { };
void _func_20_(_QWE_* x) { } // _Z9_func_20_P5_QWE_

// pointer vs reference
void _func_30_(void* x) { }                                              // _Z9_func_30_Pv
void _func_31_(int8& x) { }                                              // _Z9_func_31_Rc
void _func_32_(void** x) { }                                             // _Z9_func_32_PPv
void _func_33_(void*& x) { }                                             // _Z9_func_33_RPv
void _func_34_(void**& x) { }                                            // _Z9_func_34_RPPv
void _func_35_(void (*x)(int8)) { }                                      // _Z9_func_35_PFvcE
void _func_36_(void (*x)(int8), void (*y)(int8), void (*z)(int8)) { }    // _Z9_func_36_PFvcES0_S0_
void _func_37_(_QWE_* x) { }                                             // _Z9_func_37_P5_QWE_
void _func_38_(_QWE_* x, _QWE_* y, _QWE_* z) { }                         // _Z9_func_38_P5_QWE_S0_S0_
void _func_39_(void (*x)(_QWE_*, _QWE_*), void (*y)(_QWE_*, _QWE_*)) { } // _Z9_func_39_PFvP5_QWE_S0_ES2_

// const vs non-const
void _func_40_(void const* x) { }  // _Z9_func_40_PKv
void _func_41_(void* x) { }        // _Z9_func_41_Pv
void _func_42_(void const** x) { } // _Z9_func_42_PPKv
void _func_43_(void*const* x) { }  // _Z9_func_43_PKPv
void _func_44_(void** x) { }       // _Z9_func_44_PPv

// namespace decl
namespace _AAA_
{
    namespace _XXX_
    {
        void _func_50_() { } // _ZN5_AAA_9_func_50_Ev
    }
}

// simple class member
struct _BBB_
{
    void _func_51_() { }                             // _ZN5_BBB_9_func_51_Ev
    void _func_52_(_BBB_* x) { }                     // _ZN5_BBB_9_func_52_EPS_
    void _func_53_(_BBB_* x, _BBB_* y, _BBB_* z) { } // _ZN5_BBB_9_func_53_EPS_S0_S0_
};

// templated class member
template<class T, class T2>
struct _CCC_
{
    void _func_54_(_QWE_* x) { } // when specialize for int8 ==> _ZN5_CCC_IccE9_func_54_EP5_QWE_
};

// templated class member (specialized class)
template<>
struct _CCC_<_QWE_, _QWE_>
{
    void _func_54_(_QWE_* x) { } // _ZN5_CCC_I5_QWE_S0_E9_func_54_EPS0_
};

// class templated member
struct _DDD_
{
    template<class T, class T2>
    int64 _func_55_(_QWE_* x) { } // when specialize for int8 ==> _ZN5_DDD_9_func_55_IccEExP5_QWE_
};

// class templated member (specialized member)
template<>
int64 _DDD_::_func_55_<_QWE_, _QWE_>(_QWE_* x) { } // _ZN5_DDD_9_func_55_I5_QWE_S1_EExPS1_

// templated function
template<class T, class T2>
int64 _func_56_(_QWE_* x) { } // when specialize for int8 ==> _Z9_func_56_IccExP5_QWE_

// templated function (specialized)
template<>
int64 _func_56_<_QWE_, _QWE_>(_QWE_* x) { } // _Z9_func_56_I5_QWE_S0_ExPS0_

int main()
{
    // namespace decl
    _AAA_::_XXX_::_func_50_();

    // simple class member
    _BBB_ bbb;
    bbb._func_51_();
    bbb._func_52_(0);
    bbb._func_53_(0, 0, 0);

    // templated class member
    _CCC_<int8, int8> ccc;
    ccc._func_54_(0);

    // templated class member (specialized class)
    _CCC_<_QWE_, _QWE_> ccc2;
    ccc2._func_54_(0);

    // class templated member
    _DDD_ ddd;
    ddd._func_55_<int8, int8>(0);

    // class templated member (specialized member)
    ddd._func_55_<_QWE_, _QWE_>(0);

    // templated function
    _func_56_<int8, int8>(0);

    // templated function (specialized)
    _func_56_<_QWE_, _QWE_>(0);
}
