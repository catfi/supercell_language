function f():void
{
    var x:uint32;

    x = 0;
    x == 0;

    x && true && true;
    x || true || true;

    x + 2 + 2;
    x += 2;

    x - 2 - 2;
    x -= 2;

    x * 2 * 2;
    x *= 2;

    x / 2 / 2;
    x /= 2;

    x % 2 % 2;
    x %= 2;

    x ^ 2 ^ 2;
    x ^= 2;

    x & true & true;
    x &= true;

    x | true | true;
    x |= true;

    x << 2 << 2;
    x >> 2 >> 2;
    x <<= 2;
    x >>= 2;

    x++;
    x--;

    ++x;
    --x;
}
